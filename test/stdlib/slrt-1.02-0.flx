//Check List::_ctor_list
open List;

fun str (x:list[int]) =>
  match x with
  | Empty[int] => "()"
  | Cons (?h, ?t) => "(" + (str h) + ", " + (str t) + ")"
  endmatch
;
print$ str$ list[int](); endl;
print$ str$ list(4); endl;
print$ str$ list(4, 5); endl;
print$ str$ list(4, 5, 6); endl;
print$ str$ list(4, 5, 6, 7); endl;
