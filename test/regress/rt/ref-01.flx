//Check ref semantics

noinline proc f(var x:int, val y:int)
{
  print$ x; endl;
  ++x;
  print$ x; endl;

  print$ y; endl;
  //++y;
  //print$ y; endl;
}

call f$ 3, 1;
var a = 1;
call f$ a, a;
print "------------------\n";

inline proc g(var x:int, val y:int)
{
  print$ x; endl;
  ++x;
  print$ x; endl;

  print$ y; endl;
  //++y;
  //print$ y; endl;
}

call g$ 3, 1;
var b = 1;
call g$ b, b;

noinline proc r(x:int, ref y:int)
{
  print$ y; endl;
  y++;
  print $y; endl;
}

inline proc s(x:int, ref y:int)
{
  print$ y; endl;
  y++;
  print $y; endl;
}

a = 66;

print "------------------\n";
r$ 1, &a;
print a; endl;

print "------------------\n";
s$ 1, &a;
print a; endl;
print "------------------\n";

noinline proc h(fun y: int)
{
  print$ y; endl;
  ++a;
  print$ y; endl;
}

inline proc k(fun y: int)
{
  print$ y; endl;
  ++a;
  print$ y; endl;

}

h { a+1 };
k { a +1 };

