include "web/low_res_time";
open LowResTime;
val fmt = "%a, %d %b %Y %H:%M:%S GMT";
val rfc_1123_time = "Mon, 16 Jan 2012 14:20:07 GMT";
println(" Input: "+rfc_1123_time);
match strptime(rfc_1123_time,fmt) with
  | Some(?t) => {val new_val = strftime(fmt,t);
      	         println ("Output: "+new_val);
		}();
  | _        => println$ "Error parsing time:";
endmatch;
match strptime(rfc_1123_time,fmt) with
  | Some(?t) => {val new_val = rfc1123_date(t);
      	         println ("Output: "+new_val);
		}();
  | _        => println$ "Error parsing time:";
endmatch;


