include "std/time";

fun Tak (x:float, y:float, z:float): float =>
  if (y >= x) then z
  else Tak(Tak(x-1.0f,y,z), Tak(y-1.0f,z,x), Tak(z-1.0f,x,y))
  endif
;

val n = 10.0f;
val t0 = Time::time ();
val v = Tak(n*3.0f, n*2.0f, n*1.0f);
val t1 = Time::time ();

println$ f"%.2f" (double v);
println$ (t1 - t0);
