// Felix Performance Test Program
include "std/time";
//n := atoi(System::argv 1);
n := 13;

fun ack(x:int,y:int):int =>
  if x == 0 then y + 1
  elif y == 0 then ack(x-1, 1)
  else ack(x-1, ack(x, y-1))
  endif
;

val start_time = Time::time ();
v := ack(3,n);
println$ (Time::time ()) - start_time;
