// Felix Performance Test Program
include "std/time";

fun ack(x:int,y:int):int =>
  if x == 0 then y + 1
  elif y == 0 then ack(x-1, 1)
  else ack(x-1, ack(x, y-1))
  endif
;

val n = 13;
val t0 = Time::time ();
v := ack(3,n);
val t1 = Time::time ();

println$ f"Ack(3,%d): %d" (n, v);
println$ t1 - t0;
