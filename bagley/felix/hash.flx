include "stl";
open Stl;
open Stl::HashMap;

requires ext_string_hash;

body hexer = """
string hex(int x) { char buf[20]; snprintf(buf,20,"%x",x); return buf; }
""";

fun hex: int -> string requires hexer;

var hm = HashMap::create[string,int]();

var i = n; until i == 0 do
  insert(hm,hex i,i);
  --i;
done;

var c = 0;
i = n; until i == 0 do
  if mem (hm,str i) do c++; done;
  --i;
done;

print c; endl;
